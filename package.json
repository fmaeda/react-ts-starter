{
  "name": "ibama-ts",
  "homepage": ".",
  "version": "0.1.0",
  "private": true,
  "lint-staged": {
    "src/**/*.{js,ts,tsx,json}": [
      "prettier --single-quote --write --trailing-comma=all",
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.15",
    "@emotion/styled": "^10.0.15",
    "axios": "^0.19.0",
    "emotion-theming": "^10.0.14",
    "framer-motion": "^1.6.3",
    "immer": "^3.2.0",
    "immer-reducer": "^0.7.6",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-icons": "^3.7.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.4.1",
    "redux": "^4.0.4",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "typesafe-actions": "^4.4.2",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "stub-server": "ts-node-dev --project ./server/tsconfig.json --respawn --transpileOnly ./server/index.ts",
    "storybook": "NODE_PATH=src/ start-storybook -p 9001 -c .storybook",
    "start-react": "REACT_APP_VERSION=local.0.0 react-app-rewired start",
    "build": "REACT_APP_VERSION=\"$(git rev-parse --abbrev-ref HEAD).$(git svn info | grep Revision | egrep -o \"[0-9]+\")\" react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "start": "npm-run-all -p stub-server start-react"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@hot-loader/react-dom": "^16.8.6",
    "@types/express": "^4.17.0",
    "@types/jest": "24.0.16",
    "@types/node": "^12.7.0",
    "@types/react": "^16.9.52",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.1",
    "@types/react-router-dom": "^4.3.4",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "babel-plugin-require-context-hook": "^1.0.0",
    "body-parser": "^1.19.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.4",
    "express": "^4.17.1",
    "http-proxy-middleware": "^0.19.1",
    "husky": "^3.0.2",
    "jest-emotion": "^10.0.14",
    "lint-staged": "^9.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "react-app-rewire-hot-loader": "^2.0.1",
    "react-app-rewired": "^2.1.3",
    "react-hot-loader": "^4.12.10",
    "redux-devtools-extension": "^2.13.8",
    "ts-node-dev": "^1.0.0-pre.40"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "resolutions": {
    "@types/react": "16.8.24"
  }
}
